{
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs",
      "dest": "/dist/server.js"
    },
    {
      "src": "/api-docs.json",
      "dest": "/dist/server.js"
    },
    {
      "src": "/swagger-ui/(.*)",
      "dest": "/swagger-ui/$1"
    }
  ]
}
